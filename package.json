{
  "name": "cbmp",
  "description": "CLI for converting cursor svg files to png.",
  "author": "Abdulkaiz Khatri <kaizmandhu@gmail.com>",
  "version": "1.0.0",
  "engines": {
    "node": ">=16"
  },
  "private": false,
  "license": "MIT",
  "keywords": [
    "svg",
    "png",
    "puppeteer",
    "SMIL",
    "CSS",
    "Animation",
    "rasterize",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com:ful1e5/cbmp.git"
  },
  "bugs": {
    "url": "https://github.com/ful1e5/cbmp/issues"
  },
  "homepage": "https://github.com/ful1e5/cbmp",
  "type": "module",
  "exports": "bin/cbmp.js",
  "bin": {
    "cbmp": "bin/cbmp.js"
  },
  "scripts": {
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc -p ."
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "glob": "^10.3.4",
    "ora": "^7.0.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^21.1.1"
  },
  "devDependencies": {
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "typescript": "^5.2.2"
  }
